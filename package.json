{
	"name": "gen-package",
	"main": "index.js",
	"bin": {
		"gen-package": "index.js"
	},
	"preferGlobal": true,
	"version": "1.4.1",
	"description": "Intuitive and user-friendly generator of package.json",
	"author": {
		"email": "TiagoDanin@outlook.com",
		"name": "TiagoDanin",
		"url": "https://TiagoDanin.github.io"
	},
	"license": "MIT",
	"keywords": [
		"cli",
		"generate",
		"generator",
		"package",
		"package-json"
	],
	"scripts": {
		"start": "node index.js",
		"test": "xo"
	},
	"engines": {
		"node": ">=8"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TiagoDanin/Gen-Package.git"
	},
	"homepage": "https://TiagoDanin.github.io/Gen-Package",
	"bugs": {
		"url": "https://github.com/TiagoDanin/Gen-Package/issues"
	},
	"github": {
		"name": "Gen-Package",
		"owner": "TiagoDanin"
	},
	"files": [
		"LICENSE",
		"README.md",
		"index.js",
		"package.json"
	],
	"dependencies": {
		"choosealicense-list": "^1.1.1",
		"enquirer": "^2.3.5",
		"github-url-to-object": "4.0.4",
		"got": "^11.0.3",
		"is-online": "^8.4.0",
		"lodash": "^4.17.19",
		"minimist": "^1.2.5"
	},
	"devDependencies": {
		"xo": "^0.30.0"
	},
	"xo": {
		"rules": {
			"quote-props": "warn",
			"eqeqeq": "warn",
			"array-callback-return": "warn",
			"require-atomic-updates": "warn",
			"no-else-return": "off"
		},
		"semicolon": false
	}
}
